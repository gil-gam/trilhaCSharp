@model System.Web.Mvc.HandleErrorInfo

<h1 class="text-danger">Error.</h1>

<h2 class="text-danger">An error occurred while processing your request.</h2>

<h2 class="text-danger">Controller: @Model.ControllerName</h2>

<h2 class="text-danger">Action: @Model.ActionName</h2>

<h3 class="text-danger">Message: @Model.Exception.Message</h3>

